
actor ZEnemyCacodemon : Cacodemon {
	translation "168:191=16:47", "16:47=112:127", "112:127=160:167"
	states {
		Spawn:
		Idle:
			HEAD A 0
			HEAD A 0 ACS_NamedExecuteAlways("hate something new", 0)
			goto Super::Spawn
		ClearTarget:
			HEAD A 0 A_ClearTarget
			goto Idle
		See:
			HEAD AAAAAAAAAAAAAAA 3 A_Chase
			HEAD A 0 A_JumpIfInTargetInventory("ImDefinitelyAPlayer", 1, "ClearTarget")
			loop
		Missile:
			HEAD BC 5 A_FaceTarget
			HEAD D 5 Bright A_HeadAttack
			goto See
		Pain:
			HEAD E 3
			HEAD E 3 A_Pain
			HEAD F 6
			goto See
	}
}

