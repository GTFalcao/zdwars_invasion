actor ZEnemyCyberdemon : Cyberdemon {
  species "ZInvEnemy"
	bloodcolor "gold"
	translation "168:191=160:167", "48:63=200:207", "64:79=240:247", "144:151=200:207", "1:1=245:245"
	health 6000
	mass 5000
	painchance 4
  Damagefactor "Annihilate", 0
  Damagefactor "Ridiculous", 0.6
	-BOSS
	+QUICKTORETALIATE
  +DONTHARMSPECIES
	+NOICEDEATH
	+NODROPOFF
	+DONTRIP
	+MISSILEEVENMORE
  Dropitem "RedMana", 256
  Dropitem "RedHeroMana", 256
	states {
		Spawn:
			CYBR A 0
			CYBR A 0 A_PlaySound("cyber/sight", CHAN_WEAPON, 1.0, false, ATTN_NONE)
		Idle:
			CYBR A 0 ACS_NamedExecuteAlways("hate something new", 0)
			goto Super::Spawn
		IdleQuiet:
			CYBR A 0 ACS_NamedExecuteAlways("hate something new", 0)
			CYBR AB 10 A_LookEx(LOF_NOSEESOUND)
			goto IdleQuiet+1
		ClearTarget:
			CYBR A 0 A_ClearTarget
			goto IdleQuiet
		See:
			CYBR A 3 A_Hoof
			CYBR ABBCC 3 A_Chase
			CYBR D 3 A_Metal
			CYBR D 3 A_Chase
			CYBR A 3 A_Hoof
			CYBR ABBCC 3 A_Chase
			CYBR D 3 A_Metal
			CYBR D 3 A_Chase
			CYBR A 0 A_JumpIfInTargetInventory("ImDefinitelyAPlayer", 1, "ClearTarget")
			CYBR A 0 A_JumpIfTargetInLOS("See")
			CYBR A 0 A_Jump(64, "ClearTarget")
			loop
		Melee:
		Missile:
			CYBR E 6 A_FaceTarget
			CYBR F 10 A_CyberAttack
			CYBR E 10 A_FaceTarget
			CYBR F 10 A_CyberAttack
			CYBR E 10 A_FaceTarget
			CYBR F 10 A_CyberAttack
			goto See
		Pain:
			CYBR G 10 A_Pain
			goto See
	}
}

